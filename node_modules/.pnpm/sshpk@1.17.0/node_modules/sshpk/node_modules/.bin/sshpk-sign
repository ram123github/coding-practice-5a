#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nxtwave/nodejs/coding-practices/coding-practice-5a/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/node_modules/.pnpm/sshpk@1.17.0/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/node_modules/.pnpm/sshpk@1.17.0/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/sshpk-sign" "$@"
else
  exec node  "$basedir/../../bin/sshpk-sign" "$@"
fi
