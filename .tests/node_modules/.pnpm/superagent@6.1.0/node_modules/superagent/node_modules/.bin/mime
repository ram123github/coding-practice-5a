#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nxtwave/nodejs/coding-practices/coding-practice-5a/.tests/node_modules/.pnpm/mime@2.6.0/node_modules/mime/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/.tests/node_modules/.pnpm/mime@2.6.0/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/.tests/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/.tests/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/.tests/node_modules/.pnpm/superagent@6.1.0/node_modules/superagent/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/.tests/node_modules/.pnpm/superagent@6.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/.tests/node_modules/.pnpm/mime@2.6.0/node_modules/mime/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/.tests/node_modules/.pnpm/mime@2.6.0/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/.tests/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/.tests/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/.tests/node_modules/.pnpm/superagent@6.1.0/node_modules/superagent/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-5a/.tests/node_modules/.pnpm/superagent@6.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@2.6.0/node_modules/mime/cli.js" "$@"
else
  exec node  "$basedir/../../../../../mime@2.6.0/node_modules/mime/cli.js" "$@"
fi
